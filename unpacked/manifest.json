{
  "manifest_version": 2,
  "name": "Marcadores H&O",
  "description": "Extensión para sincronizar los marcadores del Servicio Técnico de Home & Office, no incluye el servidor.",
  "version": "0.2.0",
  "icons": {
    "24": "icon_24.png",
    "48": "icon_48.png",
    "96": "icon_96.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "bookmarks",
    "storage"
  ]
}
